<p id="notice"><%= notice %></p>

<h1>Rotating Images</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rotating_images.each do |rotating_image| %>
      <tr>
        <td><%= link_to 'Show', rotating_image %></td>
        <td><%= link_to 'Edit', edit_rotating_image_path(rotating_image) %></td>
        <td><%= link_to 'Destroy', rotating_image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rotating Image', new_rotating_image_path %>
